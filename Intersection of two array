class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        HashSet<Integer> set1= new HashSet<>();
        for(int n:nums1){
            set1.add(n);
        }

        HashSet<Integer> set2= new HashSet<>();
        for(int i:nums2){
            if(set1.contains(i)){
                set2.add(i);
            }
        }
        int result[]=new int[set2.size()];
        int index=0;
        for(int n:set2){
            result[index++]=n;
        }
        return result;


        
    }
}
