class Solution {
    public int maxLength(int[] nums) {
        final int MAX_LCM = 36_288_000; 
        int n = nums.length;
        int answer = 0;

        for (int i = 0; i < n; i++) {
            long prod = 1;
            int g = 0;
            int l = 1;

            for (int j = i; j < n; j++) {
                prod *= nums[j];
                if (prod > MAX_LCM) break;  

                g = gcd(g, nums[j]);
                l = lcm(l, nums[j]);

                if (prod == (long) g * l) {
                    answer = Math.max(answer, j - i + 1);
                }
            }
        }
        return answer;
    }
    private int gcd(int a, int b) {
        return b == 0 ? a : gcd(b, a % b);
    }
    private int lcm(int a, int b) {
        return a / gcd(a, b) * b;
    }
}
